{
    "name": "mywebapplication/epc-generator",
    "description": "Energy Performance Graph Generator",
    "keywords": [
        "my web application",
        "epc generator",
        "epc"
    ],
    "license": "MIT",
    "homepage": "https://github.com/mywebapplication/epc-generator",
    "authors": [{
        "name": "Khaled Elmahdi",
        "email": "khaled@mywebapplication.com",
        "homepage": "https://elmahdi.uk/"
    }],
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": "~5.3",
        "intervention/image": "^2.4"
    },
    "require-dev": {
        "mikey179/vfsStream": "^1.6",
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "MyWebApplication\\EpcGenerator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MyWebApplication\\EpcGenerator\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MyWebApplication\\EpcGenerator\\EpcGeneratorServiceProvider"
            ],
            "aliases": {
                "EpcGenerator": "MyWebApplication\\EpcGenerator\\EpcGeneratorFacade"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
